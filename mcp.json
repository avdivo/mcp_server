{
    "mcpServers": { 
      "info-services": {
        "command": "uv",
        "args": [
          "--directory",
          "/home/<путь к папке сервера>/mcp_server",
          "run",
          "local_server.py"
        ],
        "cwd": "/home/<путь к папке сервера>/mcp_server",
        "env": {
          "PYTHONPATH": "/home/<путь к папке сервера>/mcp_server"
        }
      }
    }
  }
  
  
  